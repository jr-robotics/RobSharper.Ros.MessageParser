<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <AssemblyName>Joanneum.Robotics.Ros.MessageParser</AssemblyName>
        <RootNamespace>Joanneum.Robotics.Ros.MessageParser</RootNamespace>
        <Title>ROS Message Parser</Title>
        <Copyright>JOANNEUM RESEARCH Forschungsgesellschaft mbH</Copyright>
        <Authors>breigo</Authors>
        <PackageLicenseExpression>BSD-3-Clause</PackageLicenseExpression>
